{
    "use_worker_mode" : true,
    "max_req_num" : 10,
    "max_task_num" : 10,
    "auto_mod_cmd" : false,
    "services" :
    [
        {
            "name" : "example",
            "service" : "github.com/yxlib/server.ExampleService",
            "mod" : 1,
            "processors" :
            [
                {
                    "name" : "Cmd1",
                    "cmd" : 1,
                    "handler" : "OnHandleCmd1",
                    "req" : "github.com/yxlib/server.Cmd1Request",
                    "resp" : "github.com/yxlib/server.Cmd1Response"
                },
                {
                    "name" : "Cmd2",
                    "cmd" : 2,
                    "handler" : "OnHandleCmd2",
                    "req" : "github.com/yxlib/server.Cmd2Request",
                    "resp" : "github.com/yxlib/server.Cmd2Response"
                }
            ]
        }
    ]
}